{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    }
  },
  "XmlSynchronizerConfig": {
    "RepeatInterval": "00:04:00",
    "IsEnabled": "true"
  },
  "XmlGeneratorConfig": {
    "RepeatInterval": "00:03:00",
    "IsEnabled": "true"
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "http://localhost:8080",
  "AutoMigrations": true
}
